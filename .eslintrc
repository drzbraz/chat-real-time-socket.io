{
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": ["error", "single"],
      "semi": ["error", "never"],
      "eol-last": "error"
    }
  }
  